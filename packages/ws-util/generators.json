{
  "$schema": "http://json-schema.org/schema",
  "name": "ws-util",
  "version": "0.0.1",
  "generators": {
    "workspace-project": {
      "factory": "./src/generators/workspace-project/generator",
      "schema": "./src/generators/workspace-project/schema.json",
      "description": "workspace-project generator"
    },
    "eslint-config-updates": {
      "factory": "./src/generators/eslint-config-updates/generator",
      "schema": "./src/generators/eslint-config-updates/schema.json",
      "description": "eslint-config-updates generator"
    },
    "workspace-initial-setup": {
      "factory": "./src/generators/workspace-initial-setup/generator",
