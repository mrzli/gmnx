{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "NodeExecutor",
  "title": "node-executor",
  "description": "Generator node-executor.",
  "examples": [
    {
      "command": "nx g @gmnx/project:node-executor",
      "description": "Execute generator node-executor."
    }
  ],
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "Full project name."
    },
    "name": {
      "type": "string",
      "description": "Node executor name."
    },
    "args": {
      "type": "string",
      "description": "Node executor args."
    }
  },
  "required": ["project", "name"]
}
